.ident.en = ":*:create documentation from source"
.ident.de = ":de:Dokumentation aus Sourcefile generieren"

Ident: lset "{.ident.en}{.ident.de}"
Version = "1.0"
Copyright = "(c) 2000 Erich Frühstück"

DocName
	:*:name of input file
	:de:Name der Eingabedatei
Output
	:*:name of output file
	:de:Name der Ausgabedatei

t|Output = "|efeudoc -t -"
	:*:terminal output
	:de:Terminalausgabe
-ps|Output = "|efeudoc -p -"
	:*:PostScript output
	:de:PostScript-Aufbereitung

n:name|Name
	:*:output name
	:de:Ausgabename
m:mode|Mode
	:*:execution mode
	:de:Verarbeitungsmodus
s:num|:eval 'Secnum="{}"'
	:*:manpage section
	:de:Handbuchabschnitt
i:fmt|:eval 'IncFmt="{}"'
	:*:format for header file
	:de:Format für Headerdateien
g|:eval 'IncFmt="<%s>"'
	:*:global header files
	:de:Globale Einbindedateien
L:lang|:eval 'setlang("{}")'
	:*:set language
	:de:Sprachumgebung
l|:eval 'ListMode(iostd); exit(0);'
	:*:list execution modes 
	:de:Verarbeitungsmodi auflisten
:src|:eval 'DocName="{}"'
	:*:input file
	:de:Eingabefile
::out|Output
	:*:output file
	:de:Ausgabefile
