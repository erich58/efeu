#include <d2m.cfg>

#undef	SubMake
#define	SubMake(name,label) \
SubMakeRule(name,label,name,NullArg,dir2make)

#if	LANG_de
SubMake(make,"make und co")
SubMake(prog,"EFEU Programmierhandbuch")
SubMake(mdmat,"Handbuch zu den Datenmatrizen")
SubMake(efeudoc,"Handbuch zum Dokumentkonverter")
SubMake(user,"Benutzerhandbuch")
SubMake(man1,"Handbucheinträge zu Kommandos")
#else
SubMake(make,"german documentation about mkmf")
SubMake(prog,"german documentation about EFEU programming")
SubMake(mdmat,"german documentation about data cubes")
SubMake(efeudoc,"german documentation about efeudoc")
SubMake(user,"user reference manual")
SubMake(man1,"manpages for commands")
#endif
