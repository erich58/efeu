#include <d2m.cfg>

TOPDIR = EFEUROOT
CFGDIR = $(TOPDIR)/ppinclude
INCDIR = $(TOPDIR)/include/GUI
BINDIR = $(TOPDIR)/bin
LIBDIR = $(TOPDIR)/lib
APPDIR = $(TOPDIR)/lib/efeu

DIR2MAKE = dir2make -H $(INCDIR) -B $(BINDIR) -L $(LIBDIR) -A $(APPDIR)

SubMake(letters,"EFEU Letters and Numbers",letters,NullArg,NullArg)

